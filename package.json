{
  "name": "@apimda/s3-utils",
  "version": "0.0.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test": {
      "types": "./dist/test/index.d.ts",
      "default": "./dist/test/index.js"
    }
  },
  "main": "dist/index.js",
  "files": [
    "/dist",
    "!/**/*.map",
    "!/**/*.test.*"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "format": "prettier --plugin-search-dir . --write . && prettier-package-json --write",
    "preinstall": "npx only-allow pnpm",
    "lint": "prettier --plugin-search-dir . --check . && eslint \".\"",
    "playground": "pnpm --filter integration-test run playground",
    "prepare": "husky install",
    "repo-clean": "git clean -xdf",
    "test": "pnpm run build && vitest run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.188.0",
    "@aws-sdk/s3-request-presigner": "^3.188.0"
  },
  "peerDependencies": {
    "testcontainers": "^9.8.0"
  },
  "peerDependenciesMeta": {
    "testcontainers": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/node": "^18.16.5",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "axios": "^1.4.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-package-json": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "keywords": [
    "aws",
    "s3"
  ],
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "package.json": "prettier-package-json --write",
    "*.ts": "eslint"
  },
  "packageManager": "pnpm@8.6.0"
}
